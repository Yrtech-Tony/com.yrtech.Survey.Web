@{
    ViewBag.Title = "巡检系统-品牌管理";
}

<div class="page-title">
    <div class="title_left">
        <h3>品牌管理 <small></small></h3>
    </div>
</div>
<table id="brand-table" class="table table-striped table-bordered dt-responsive">
    <thead>
        <tr>
            @*<th><input type="checkbox" id="check-all" class="flat"></th>*@
            <th width="100">品牌编号</th>
            <th width="150">品牌名称</th>            
            <th width="250">备注</th>
            @*<th width="100">添加人</th>*@
            <th width="150">添加时间</th>
            @*<th width="100">修改人</th>*@
            <th width="150">最新修改时间</th>
            <th width="150">账号管理</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>
<div id="pagination" class="pull-right">
    <ul id="pageUl"></ul>
</div>
<div class="modal bs-example-modal-lg fade" id="Modal" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">账号管理</h4>
            </div>
            <div class="modal-body">
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/Scripts/api.js"></script>
    <script src="~/Scripts/page.js"></script>
    <script>
        $(function () {
            loadBrand();
        })
    </script>
}



