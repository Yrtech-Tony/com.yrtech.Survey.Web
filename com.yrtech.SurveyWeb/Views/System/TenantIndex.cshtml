}
<div class="page-title">
    <div class="title_left">
        <h3>租户管理 <small></small></h3>
    </div>
</div>
<table id="tenant-table" class="table table-striped table-bordered dt-responsive">
    <thead>
        <tr>
            @*<th><input type="checkbox" id="check-all" class="flat"></th>*@
            <th>租户编码</th>
            <th>租户名称</th>
            <th>邮箱</th>
            <th>电话</th>
            @*<th>添加人</th>*@
            <th>添加时间</th>
            @*<th>修改人</th>*@
            <th>最新修改时间</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>
<div id="pagination" class="pull-right">
    <script src="~/Scripts/page.js"></script>
    <ul id="pageUl"></ul>
</div>

@section scripts{
    <script src="~/Scripts/api.js"></script>
    <script src="~/Scripts/page.js"></script>
    <script>
        $(function () {
            loadTenant();
        })
    </script>
}