@{
    ViewBag.Title = "巡检系统-期号管理";
}

<div class="page-title">
    <div class="title_left">
        <h3>期号管理 <small></small></h3>
    </div>
</div>
<table id="project-table" class="table table-striped table-bordered dt-responsive">
    <thead>
        <tr>
            <th><input type="checkbox" id="check-all" class="flat"></th>
            <th>期号编码</th>
            <th>期号名称</th>
            <th>年份</th>
            <th>季度</th>
            <th>序号</th>
            @*<th>添加人</th>*@
            <th>添加时间</th>
            @*<th>修改人</th>*@
            <th>最新修改时间</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>
@section scripts{
    <script src="~/Scripts/api.js"></script>
<script src="~/Scripts/page.js"></script>
    <script>
        $(function () {
            loadProject();
        })
    </script>
}



