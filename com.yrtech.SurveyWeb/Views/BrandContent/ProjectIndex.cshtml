}

<div class="page-title">
    <div class="title_left">
        <h3>期号管理 <small></small></h3>
    </div>
</div>
<table id="project-table" class="table table-striped table-bordered dt-responsive">
    <thead>
        <tr>
            @*<th><input type="checkbox" id="check-all" class="flat"></th>*@
            <th width="100">期号编号</th>
            <th>期号名称</th>
            <th width="80">年份</th>
            <th width="80">季度</th>
            <th width="80">序号</th>
            <th width="100">添加人</th>
            <th width="150">添加时间</th>
            <th width="100">修改人</th>
            <th width="150">最新修改时间</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>
<div id="pagination" class="pull-right">
    <ul id="pageUl"></ul>
</div>

@section scripts{
    <script src="~/Scripts/api.js"></script>
<script src="~/Scripts/page.js"></script>
